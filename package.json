{
    "name": "m-lens",
    "version": "2.0.1",
    "private": true,
    "type": "module",
    "dependencies": {
        "@ant-design/icons": "^6.0.2",
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@react-google-maps/api": "^2.20.7",
        "@reduxjs/toolkit": "^2.9.0",
        "@tailwindcss/vite": "^4.1.13",
        "@types/react": "^19.1.13",
        "@vercel/analytics": "^1.5.0",
        "@vercel/speed-insights": "^1.2.0",
        "antd": "^5.27.4",
        "apexcharts": "^5.3.5",
        "dayjs": "^1.11.18",
        "firebase": "^12.3.0",
        "lucide-react": "^0.526.0",
        "react": "^19.1.1",
        "react-apexcharts": "^1.7.0",
        "react-countdown": "^2.3.6",
        "react-countup": "^6.5.3",
        "react-csv": "^2.2.2",
        "react-diff-viewer-continued": "^3.4.0",
        "react-dom": "^19.1.1",
        "react-helmet-async": "^2.0.5",
        "react-highlight-words": "^0.21.0",
        "react-multi-select-component": "^4.3.4",
        "react-redux": "^9.2.0",
        "react-router-dom": "^7.9.2",
        "react-top-loading-bar": "^3.0.2",
        "recharts": "^3.2.1",
        "rimraf": "^6.0.1",
        "tailwindcss": "^4.1.13",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:clean": "rimraf dist node_modules/.cache && vite build",
        "build:analyze": "ANALYZE=true vite build",
        "preview": "vite preview",
        "clean": "rimraf dist .vite",
        "build:nocache": "vite build --emptyOutDir",
        "lint": "eslint .",
        "lint:errors": "eslint . --quiet",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
        "prepare": "husky",
        "test": "jest",
        "pre-commit": "lint-staged",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-react": "^7.27.1",
        "@eslint/js": "^9.36.0",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^4.7.0",
        "eslint": "^9.36.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-unused-imports": "^4.2.0",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^16.2.0",
        "prettier": "^3.6.2",
        "rollup-plugin-visualizer": "^6.0.3",
        "terser": "^5.44.0",
        "vite": "^7.1.7",
        "vite-plugin-compression": "^0.5.1"
    },
    "pnpm": {
        "overrides": {
            "react-diff-viewer-continued>react": "^19.1.0",
            "react-diff-viewer-continued>react-dom": "^19.1.0",
            "react-helmet-async>react": "^19.1.0",
            "react-multi-select-component>react": "^19.1.0",
            "react-multi-select-component>react-dom": "^19.1.0"
        },
        "onlyBuiltDependencies": [
            "@tailwindcss/oxide"
        ]
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "prettier --write",
            "eslint --fix"
        ],
        "*.{json,css,md}": [
            "prettier --write"
        ]
    },
    "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.js"
        ],
        "moduleNameMapping": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/index.js",
            "!src/reportWebVitals.js",
            "!src/**/*.stories.{js,jsx}",
            "!src/**/__tests__/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 70,
                "functions": 70,
                "lines": 70,
                "statements": 70
            }
        },
        "transform": {
            "^.+\\.(js|jsx)$": [
                "babel-jest",
                {
                    "presets": [
                        [
                            "@babel/preset-env",
                            {
                                "targets": {
                                    "node": "current"
                                }
                            }
                        ],
                        [
                            "@babel/preset-react",
                            {
                                "runtime": "automatic"
                            }
                        ]
                    ]
                }
            ]
        },
        "moduleFileExtensions": [
            "js",
            "jsx",
            "json"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
            "<rootDir>/src/**/*.{test,spec}.{js,jsx}"
        ]
    }
}
